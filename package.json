{
  "name": "react-admin-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-model:dev": "cd ./Samples/TypeScript/Demo/ && npm run-script build",
    "build-model:prod": "cd ./Samples/TypeScript/Demo/ && npm run-script build:prod",
    "prettier": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "lint": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:snap": "vitest -u",
    "lint-staged": "yarn prettier && yarn lint && tsc --noEmit",
    "prepare": "husky install",
    "format": "yarn prettier && yarn lint:fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.0",
    "axios": "^1.4.0",
    "chart.js": "^4.4.1",
    "i18next": "^23.4.9",
    "moment": "^2.29.4",
    "react": "^18.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "^18.0.0",
    "react-easy-crop": "^4.7.4",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.45.4",
    "react-i18next": "^13.2.1",
    "react-player": "^2.12.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0",
    "recoil": "^0.7.7",
    "socket.io-client": "^4.7.2",
    "vite-plugin-compression": "^0.5.1",
    "whatwg-fetch": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.4",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/react": "^18.2.21",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "happy-dom": "^4.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.28",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.1",
    "serve": "^14.2.0",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^6.2.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^4.15.1"
  },
  "volta": {
    "node": "18.19.0"
  }
}
